{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": ["SANDBOX_AGENT_SKIP_INSPECTOR", "SKIP_OPENAPI_GEN", "NIX_*", "SDKROOT", "DEVELOPER_DIR", "MACOSX_DEPLOYMENT_TARGET"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", "lib/**", "src/generated/**"]
    },
    "generate": {
      "dependsOn": ["^generate"],
      "outputs": ["src/generated/**"]
    },
    "typecheck": {
      "dependsOn": ["^build", "build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
